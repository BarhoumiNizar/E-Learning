  <!-- Start latest course section -->
  <section id="mu-latest-courses">
    <div class="container">
      <div class="row">
        <div class="col-lg-12 col-md-12">
          <div class="mu-latest-courses-area">
            <!-- Start Title -->
            <div class="mu-title">
              <h2>Nos Evénements</h2>
              <p>Retrouvez toutes nos actualités.
                <br>Découvrez ci-dessous nos évenements à venir.<br>
                Cliquer pour s'inscrire.
              </p>
            </div>
            <!-- End Title -->
            <!-- Start latest course content -->
            <div id="mu-latest-course-slide" class="mu-latest-courses-content">
                <% events.forEach((event, index) => {  %> 
                
              <div class="col-lg-4 col-md-4 col-xs-12">
                <div class="mu-latest-course-single">
                  <figure class="mu-latest-course-img">
                    <a href="inscription/<%=event.NomEvent%>"><img src=" img/courses/1.jpg" alt="img"></a>
                    <figcaption class="mu-latest-course-imgcaption">
                     <a href="#"><%= event.NomEvent %> </a>
                      <span><i class="fa fa-clock-o"></i><%= event.Duree %> heures</span>
                    </figcaption>
                  </figure>
                  <div class="mu-latest-course-single-content">
                    <h4><a href="#"><%= event.objet %></a></h4>
                    
                    <div class="mu-latest-course-single-contbottom">
                      <a class="mu-course-details" href="/detailEvent/<%= event.IDevent %>" >Details</a>
                      <span class="mu-course-price" href="#"><%= event.Prix_Event %> DT</span>
                    </div>
                  </div>
                </div>
              </div>

           
              <% }) %>

            
              
              
          
            </div>
            <!-- End latest course content -->
          </div>
        </div>
      </div>
    </div>
  </section>
